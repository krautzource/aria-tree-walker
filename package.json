{
  "name": "@krautzource/aria-tree-walker",
  "version": "1.0.0",
  "description": "A lightweight walker for labeled aria(-owns) trees",
  "main": "lib/navigator.mjs",
  "scripts": {
    "pretest": "npx http-server . &",
    "posttest": "kill $(ps aux | grep '[h]ttp-server' | awk '{print $2}')",
    "test": "ava",
    "build": "npx rollup --file dist/treewalker.js -f esm lib/treewalker.mjs",
    "docs": "node docs/source/generateDocs.js && npx rollup --file docs/treewalker.js -f esm lib/navigator.mjs",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krautzource/aria-tree-walker.git"
  },
  "keywords": [
    "accessibility",
    "aria tree",
    "aria owns",
    "tree walker",
    "diagrams",
    "accessible diagram",
    "accessible equation",
    "speech-rule-engine",
    "mathjax"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/krautzource/aria-tree-walker/issues"
  },
  "homepage": "https://github.com/krautzource/aria-tree-walker#readme",
  "devDependencies": {
    "ava": "^3.15.0",
    "http-server": "^0.12.3",
    "jsdom": "^16.4.0",
    "mathjax-full": "^3.1.2",
    "playwright": "^1.8.0",
    "rollup": "^2.28.2",
    "speech-rule-engine": "^3.1.1",
    "sre-to-tree": "github:krautzource/sre-to-tree",
    "standard-version": "^9.0.0"
  }
}
